        "@tailwindcss/vite": "^4.1.18",
        "daisyui": "^5.5.14",
        "gh-pages": "^6.3.0",
        "prettier": "^3.8.1",
        "prettier-plugin-svelte": "^3.4.1",
        "prettier-plugin-tailwindcss": "^0.7.2",
        "svelte": "^5.48.2",
        "svelte-check": "^4.3.5",
        "tailwindcss": "^4.1.18",
        "typescript": "^5.9.3",
        "vite": "^7.3.1"
    },
